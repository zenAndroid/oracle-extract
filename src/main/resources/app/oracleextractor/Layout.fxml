<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="800.0" minWidth="1300.0" prefHeight="700.0"
      prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="app.oracleextractor.MainController">
    <ButtonBar buttonMinWidth="175.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="loadImpreciseButton" mnemonicParsing="false" text="Load an imprecise oracle"/>
            <Button fx:id="loadTestSuiteButton" mnemonicParsing="false" text="Load initial test suite"/>
            <Button fx:id="startMiningButton" mnemonicParsing="false" text="Start mining"/>
            <Button fx:id="stopMiningButton" mnemonicParsing="false" text="Stop mining"/>
        </buttons>
        <VBox.margin>
            <Insets/>
        </VBox.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
    </ButtonBar>
    <TabPane VBox.vgrow="ALWAYS">
        <Tab closable="false" text="UI">
            <GridPane prefHeight="490.0" prefWidth="828.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308"
                            maxWidth="1.7976931348623157E308" text="Imprecise Oracle"
                            GridPane.columnSpan="2">
                    <VBox prefHeight="200.0" prefWidth="100.0">
                        <WebView fx:id="impreciseOracleView" prefHeight="200.0" prefWidth="200.0"
                                 VBox.vgrow="ALWAYS"/>
                        <ButtonBar buttonMinWidth="150.0" prefHeight="40.0" prefWidth="686.0">
                            <buttons>
                                <Button fx:id="zoomOutImpreciseButton" mnemonicParsing="false"
                                        text="Zoom out"/>
                                <Button fx:id="zoomResetImpreciseButton" mnemonicParsing="false"
                                        text="Reset zoom level"/>
                                <Button fx:id="zoomInImpreciseButton" mnemonicParsing="false"
                                        text="Zoom in"/>
                            </buttons>
                        </ButtonBar>
                    </VBox>
                </TitledPane>
                <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308"
                            maxWidth="1.7976931348623157E308" prefHeight="342.0" prefWidth="707.0"
                            text="Partial Oracle" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <VBox prefHeight="200.0" prefWidth="100.0">
                        <WebView fx:id="partialOracleView" prefHeight="200.0" prefWidth="200.0"
                                 VBox.vgrow="ALWAYS"/>
                        <ButtonBar buttonMinWidth="150.0" prefHeight="40.0" prefWidth="200.0">
                            <buttons>
                                <Button fx:id="zoomOutPartialButton" mnemonicParsing="false"
                                        text="Zoom out"/>
                                <Button fx:id="zoomResetPartialButton" mnemonicParsing="false"
                                        text="Reset zoom level"/>
                                <Button fx:id="zoomInPartialButton" mnemonicParsing="false"
                                        text="Zoom in"/>
                            </buttons>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                        </ButtonBar>
                    </VBox>
                </TitledPane>
                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowSpan="2">
                    <TitledPane animated="false" collapsible="false" text="Input sequence">
                        <HBox prefHeight="14.0" prefWidth="245.0">
                            <Label fx:id="inSeq" text="Label"/>
                        </HBox>
                    </TitledPane>
                    <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" text="Plausible output sequences"
                                VBox.vgrow="ALWAYS">
                        <VBox prefHeight="200.0" prefWidth="100.0">
                            <ListView fx:id="outputSeqList" maxHeight="1.7976931348623157E308"
                                      maxWidth="1.7976931348623157E308" prefHeight="200.0"
                                      prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                            <ButtonBar prefHeight="40.0" prefWidth="200.0">
                                <buttons>
                                    <Button fx:id="proceedButton" mnemonicParsing="false"
                                            text="Proceed with selected output sequence"/>
                                </buttons>
                            </ButtonBar>
                        </VBox>
                    </TitledPane>
                    <TitledPane animated="false" collapsible="false" text="Initial test suite">
                        <HBox prefHeight="18.0" prefWidth="245.0">
                            <ComboBox prefWidth="150.0"/>
                        </HBox>
                    </TitledPane>
                </VBox>
            </GridPane>
        </Tab>
        <Tab closable="false" text="Execution log">
            <VBox prefHeight="200.0" prefWidth="100.0">
                <TextArea fx:id="executionLogTextArea" editable="false" prefHeight="200.0" prefWidth="200.0"
                          VBox.vgrow="ALWAYS"/>
                <ButtonBar buttonMinWidth="175.0" prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="clearLogButton" mnemonicParsing="false" text="Clear log"/>
                    </buttons>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </ButtonBar>
            </VBox>
        </Tab>
    </TabPane>
</VBox>
